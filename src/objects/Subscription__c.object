    <fields>
        <fullName>Calc_Prior_Month_Revenue__c</fullName>
        <description>Calculated Prior Month  Revenue</description>
        <externalId>false</externalId>
        <formula>IF( 
(MONTH( PurchaseDate__c ) = ((MONTH(TODAY()) -1) -1) &amp;&amp; (YEAR( PurchaseDate__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) ) 
&amp;&amp; (MONTH( Previous_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR (TODAY()) )), 
MonthlyRecurringRevenue__c , 

IF( 
(MONTH( PurchaseDate__c ) = (MONTH(TODAY()) -1) &amp;&amp; (YEAR( PurchaseDate__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) )), 
MonthlyRecurringRevenue__c, 

IF( 
(MONTH( Previous_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) ), 
Previous_Upgrade_Amount__c + X1st_Upgrade_Amount__c, 

IF( 
MONTH( X1st_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) 
, X1st_Upgrade_Amount__c , 

IF( 
MONTH( PurchaseDate__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( PurchaseDate__c ) = YEAR (TODAY()) , 
MonthlyRecurringRevenue__c , 

IF( 
MONTH( Previous_Upgrade_Date__c ) = (MONTH(TODAY()) -1) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR (TODAY()) , Previous_Upgrade_Amount__c, 

IF( Strategic_Partner_Type__c = &quot;Reseller&quot; &amp;&amp; ((MONTH(TODAY()) -1) = MONTH( StartDate__c )&amp;&amp; ( YEAR( TODAY()) = YEAR( StartDate__c ) )) , SBD_Value__c , 
NULL) 
))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>zCalc Prior Month Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
