    <fields>
        <fullName>Is_Cancelled__c</fullName>
        <description>Formula converting sub status to a TRUE checkbox if &lt;&gt; &quot;Active&quot; so it can be summarized in reports</description>
        <externalId>false</externalId>
        <formula>OR(
ISPICKVAL( Status__c , &quot;Cancelled&quot;),
ISPICKVAL( Status__c , &quot;Terminated&quot;),
ISPICKVAL( Status__c , &quot;Incomplete&quot;),
ISPICKVAL( Status__c , &quot;Fraud&quot;),
ISPICKVAL( Status__c , &quot;Pending&quot;),
ISPICKVAL( Status__c , &quot;Suspended&quot;)
)</formula>
        <inlineHelpText>Is the sub status cancelled, terminated, incomplete, fraud, pending, or suspended?</inlineHelpText>
        <label>Is Cancelled</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
