IF( ProductID__c=&quot;57&quot;, 14.99,
IF( ProductID__c=&quot;58&quot;, 29.99,
IF( ProductID__c=&quot;101&quot;, 29.95,
IF( ProductID__c=&quot;102&quot;, 79.95,
IF( ProductID__c=&quot;103&quot;, 199.95,
NULL)))))))))))
*/
CASE(ProductID__c,
&quot;49&quot;, 24.95,
&quot;50&quot;, 39.95,
&quot;97&quot;, 29.95,
&quot;98&quot;, 79.95,
&quot;99&quot;, 199.95,
&quot;52&quot;, 149.95,
&quot;53&quot;, 299.95,
&quot;57&quot;, 14.99,
&quot;58&quot;, 29.99,
&quot;101&quot;, 29.95,
&quot;102&quot;, 79.95,
&quot;103&quot;, 199.95,
&quot;115&quot;, 79.95,
null)
)</formula>
