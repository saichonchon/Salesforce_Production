    <fields>
        <fullName>BI_This_Month_s_Revenue__c</fullName>
        <description>I only updated the first two if statements - MH</description>
        <externalId>false</externalId>
        <formula>IF( 
((MONTH( BI_Date_Paid__c ) = MONTH(TODAY()) &amp;&amp; (YEAR( BI_Date_Paid__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) ) 
&amp;&amp; (MONTH( Previous_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR (TODAY()) )) &amp;&amp; ISPICKVAL (BI_Invoice_Status__c, &quot;Paid&quot;)), 
BI_Payment_Amount__c , 

IF( 
((MONTH( BI_Date_Paid__c ) = MONTH(TODAY()) &amp;&amp; (YEAR( BI_Date_Paid__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) )) &amp;&amp; 
ISPICKVAL (BI_Invoice_Status__c, &quot;Paid&quot;)), 
BI_Payment_Amount__c, 

IF( 
(MONTH( Previous_Upgrade_Date__c ) = MONTH(TODAY()) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR(TODAY()) ) 
&amp;&amp; (MONTH( X1st_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) ), 
Previous_Upgrade_Amount__c + X1st_Upgrade_Amount__c, 

IF( 
MONTH( X1st_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( X1st_Upgrade_Date__c ) = YEAR (TODAY()) 
, X1st_Upgrade_Amount__c , 

IF( 
MONTH( BI_Date_Paid__c ) = MONTH (TODAY()) &amp;&amp; YEAR( BI_Date_Paid__c ) = YEAR (TODAY()) , 
BI_Payment_Amount__c , 

IF( 
MONTH( Previous_Upgrade_Date__c ) = MONTH (TODAY()) &amp;&amp; YEAR( Previous_Upgrade_Date__c ) = YEAR (TODAY()) , Previous_Upgrade_Amount__c, 

IF( Strategic_Partner_Type__c = &quot;Reseller&quot; &amp;&amp; (MONTH(TODAY()) = MONTH( StartDate__c )&amp;&amp; ( YEAR( TODAY()) = YEAR( StartDate__c ) )) , SBD_Value__c , 
NULL) 
))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>zBI This Month&apos;s Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
