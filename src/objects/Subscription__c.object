        <formula>IF(  OR(Strategic_Partner_Type__c  &lt;&gt;&quot;Reseller&quot;,Referral_ID__c = &quot;heartland_referral&quot;),MonthlyRecurringRevenue__c,
/*
IF( ProductID__c=&quot;49&quot;, 24.95,
IF( ProductID__c=&quot;50&quot;, 39.95,
IF( ProductID__c=&quot;98&quot;, 79.95,
IF( ProductID__c=&quot;52&quot;, 149.95,
