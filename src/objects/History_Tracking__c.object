<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Holds the fields for specific objects that will be tracked in custom history objects.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Marks the current field history tracking record as actively tracked or not.  If not active the field will not be tracked in history</description>
        <externalId>false</externalId>
        <inlineHelpText>Marks the current field history tracking record as actively tracked or not.  If not active the field will not be tracked in history</inlineHelpText>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Field_Name_API__c</fullName>
        <description>The API name of the Field for the associated object to track</description>
        <externalId>false</externalId>
        <inlineHelpText>The API name of the Field for the associated object to track</inlineHelpText>
        <label>Field Name (API)</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Object_Name_API__c</fullName>
        <description>The API name of the Object for the associated field to track</description>
        <externalId>false</externalId>
        <inlineHelpText>The API name of the Object for the associated field to track</inlineHelpText>
        <label>Object Name (API)</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>History Tracking</label>
    <visibility>Protected</visibility>
</CustomObject>
